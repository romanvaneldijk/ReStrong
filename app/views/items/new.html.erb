<%= simple_form_for @item do |f| %>

  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.submit %>
<% end %>

  t.float "price"
    t.text "description"
    t.string "name"
    t.datetime "created_at", precision: 6, null: false
    t.datetime "updated_at", precision: 6, null: false
    t.string "imageurl"
